{
  "name": "checklist",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "nodemon": "^1.11.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router": "^4.1.2",
    "react-scripts": "1.0.10",
    "react-transition-group": "^1.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1"
  },
  "scripts": {
    "build": "set NODE_ENV=production&& react-scripts build && babel api -d server",
    "start": "set NODE_ENV=production&& node ./server/index.js",
	"start-test": "set NODE_ENV=production&& nodemon ./api/index.js --exec babel-node",
    "go": "npm run build && npm run start",
    "server": "set NODE_ENV=development&& nodemon ./api/index.js --exec babel-node",
    "server-build": "babel api -d server",
    "client": "react-scripts start",
    "client-build": "react-scripts build",
    "client-test": "react-scripts test --env=jsdom"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  }
}
